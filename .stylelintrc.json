{
  "extends": ["stylelint-config-recommended", "stylelint-config-recess-order"],
  "customSyntax": "postcss-styled-syntax",
  "rules": {
    "property-no-unknown": true,
    "declaration-block-no-duplicate-properties": true,
    "value-keyword-case": [
      "lower",
      {
        "camelCaseSvgKeywords": true
      }
    ]
  }
}
